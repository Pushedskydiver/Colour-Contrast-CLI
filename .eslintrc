{
  "env": {
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6
  },
  "globals": {
    "document": true,
    "window": true
  },
  "rules": {
    "comma-dangle": [
      2,
      "never"
    ],
    "no-cond-assign": [
      2,
      "always"
    ],
    "no-console": 2,
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 1,
    "no-dupe-args": 1,
    "no-dupe-keys": 2,
    "no-duplicate-case": 1,
    "no-empty-character-class": 1,
    "no-empty": 2,
    "no-ex-assign": 1,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": [
      2,
      "all"
    ],
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": [
      2,
      "functions"
    ],
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-negated-in-lhs": 1,
    "no-obj-calls": 2,
    "no-regex-spaces": 2,
    "no-sparse-arrays": 2,
    "no-unreachable": 2,
    "use-isnan": 2,
    "valid-typeof": 1,
    "no-unexpected-multiline": 2,
    "block-scoped-var": 1,
    "complexity": [
      2,
      4
    ],
    "consistent-return": 2,
    "curly": [
      2,
      "all"
    ],
    "eqeqeq": [
      2,
      "smart"
    ],
    "guard-for-in": 1,
    "no-alert": 2,
    "no-eval": 2,
    "no-floating-decimal": 2,
    "no-invalid-this": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "vars-on-top": 2,
    "strict": [
      2,
      "global"
    ],
    "no-undef": 2,
    "no-undefined": 2,
    "no-unused-vars": 2,
    "no-use-before-define": [
      2,
      "nofunc"
    ],
    "camelcase": [
      2,
      {
        "properties": "always"
      }
    ],
    "comma-spacing": [
      2,
      {
        "after": true
      }
    ],
    "func-names": [
      1,
      "as-needed"
    ],
    "no-trailing-spaces": [
      2,
      {
        "skipBlankLines": false
      }
    ],
    "space-before-blocks": [
      2,
      "always"
    ],
    "space-unary-ops": [
      2,
      {
        "words": true,
        "nonwords": false
      }
    ],
    "arrow-parens": [
      2,
      "as-needed"
    ],
    "arrow-spacing": [
      2,
      {
        "before": true,
        "after": true
      }
    ],
    "no-var": 2,
    "prefer-const": 1,
    "prefer-spread": 1,
    "max-depth": [
      1,
      4
    ],
    "max-len": [
      1,
      100,
      4,
      {
        "ignorePattern": "true",
        "ignoreComments": true,
        "ignoreUrls": true
      }
    ],
    "max-params": [
      1,
      3
    ],
    "max-statements": [
      1,
      10
    ]
  }
}